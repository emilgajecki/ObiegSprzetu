<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="1080.0" prefWidth="1900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.NewUserController">
    <children>
      <Label layoutX="775.0" layoutY="132.0" prefHeight="30.0" prefWidth="254.0" text="Dodaj nowego użytkownika" textAlignment="CENTER" textFill="#f16823">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="649.0" layoutY="205.0" prefHeight="30.0" prefWidth="206.0" text="Imie uzytkownika" textFill="#f16823">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="649.0" layoutY="254.0" prefHeight="30.0" prefWidth="206.0" text="Nazwisko użytkownika" textFill="#f16823">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField layoutX="872.0" layoutY="208.0" onAction="#getName" prefHeight="25.0" prefWidth="228.0" />
      <TextField layoutX="872.0" layoutY="257.0" onAction="#getLastName" prefHeight="25.0" prefWidth="228.0" />
      <Button layoutX="889.0" layoutY="331.0" mnemonicParsing="false" onAction="#addUserToDatabase" text="Dodaj użytkownika" textFill="#f16823">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
        <MenuBar id="menu" layoutY="2.0" prefHeight="25.0" prefWidth="306.0">
            <menus>
                <Menu mnemonicParsing="false" text="Nowy">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#newUser" text="Użytkownik" />
                        <MenuItem mnemonicParsing="false" onAction="#newDevice" text="Sprzęt" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Pokaż">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#seeUser" text="Użytkowników" />
                        <MenuItem mnemonicParsing="false" onAction="#seeDevice" text="Sprzęt" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Sprawdź">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#checkUser" text="Użytkownika" />
                        <MenuItem mnemonicParsing="false" onAction="#checkDevice" text="Sprzęt" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Zablokuj">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#blockUser" text="Użytkownika" />
                        <MenuItem mnemonicParsing="false" onAction="#blockDevice" text="Sprzęt" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" onAction="#closeApp" text="Zamknij">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#closeApp" text="Zamknij aplikacje" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</Pane>
