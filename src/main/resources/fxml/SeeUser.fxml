<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="1080.0" prefWidth="1900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SeeUserController">
    <children>
        <Label layoutX="582.0" layoutY="126.0" prefHeight="30.0" prefWidth="195.0" text="Sprawdź użytkownika " textAlignment="CENTER" textFill="#f16823">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Label layoutX="479.0" layoutY="255.0" prefHeight="30.0" prefWidth="206.0" text="Nazwisko użytkownika" textFill="#f16823">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <TextField layoutX="731.0" layoutY="209.0" onAction="#getName" prefHeight="25.0" prefWidth="228.0" />
        <TextField layoutX="731.0" layoutY="258.0" onAction="#getLastName" prefHeight="25.0" prefWidth="228.0" />
        <Button layoutX="731.0" layoutY="327.0" mnemonicParsing="false" onAction="#checkUserInDatabase" text="Sprawdź użytkownika" textFill="#f16823">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <Label layoutX="479.0" layoutY="206.0" prefHeight="30.0" prefWidth="162.0" text="Imie uzytkownika" textFill="#f16823">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
      <Pane layoutX="15.0" layoutY="186.0" prefHeight="325.0" prefWidth="575.0" />
        <MenuBar id="menu" layoutY="2.0" prefHeight="25.0" prefWidth="306.0">
            <menus>
                <Menu mnemonicParsing="false" text="Nowy">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#newUser" text="Użytkownik" />
                        <MenuItem mnemonicParsing="false" onAction="#newDevice" text="Sprzęt" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Pokaż">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#seeUser" text="Użytkowników" />
                        <MenuItem mnemonicParsing="false" onAction="#seeDevice" text="Sprzęt" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Sprawdź">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#checkUser" text="Użytkownika" />
                        <MenuItem mnemonicParsing="false" onAction="#checkDevice" text="Sprzęt" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Obieg sprzętu" >
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#hardwareRelease" text="Wydanie sprzętu" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Zablokuj">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#blockUser" text="Użytkownika" />
                        <MenuItem mnemonicParsing="false" onAction="#blockDevice" text="Sprzęt" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" onAction="#closeApp" text="Zamknij">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#closeApp" text="Zamknij aplikacje" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</Pane>
